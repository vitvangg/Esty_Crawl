{
    "manifest_version": 3,
    "name": "Etsy Search Titles Scraper",
    "version": "1.0",
    "description": "Scrape product titles from Etsy search page",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  